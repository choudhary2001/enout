{
  "expo": {
    "name": "Enout Mobile",
    "slug": "enout-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "scheme": "enout-mobile",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F9B24E"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.enout.mobile"
    },
    "android": {
      "package": "com.enout.mobile",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F9B24E"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 24,
            "ndkVersion": "26.1.10909125"
          }
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "70fe7d38-d541-4e49-8885-27d2779df11d"
      }
    },
    "owner": "divakarchoudhary"
  }
}