{
  "name": "mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "packageManager": "npm@10.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android --profile preview",
    "build:android:prod": "eas build --platform android --profile production",
    "build:apk": "eas build --platform android --profile preview --local",
    "build:apk:prod": "eas build --platform android --profile production --local",
    "build:aab": "eas build --platform android --profile production-aab --local",
    "build:aab:remote": "eas build --platform android --profile production-aab",
    "build:all": "npm run build:apk:prod && npm run build:aab"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.90.2",
    "expo": "~54.0.10",
    "expo-constants": "~18.0.10",
    "expo-document-picker": "^14.0.7",
    "expo-build-properties": "~1.0.9",
    "expo-image-picker": "^17.0.8",
    "expo-linking": "^8.0.8",
    "expo-router": "~6.0.8",
    "expo-secure-store": "^15.0.7",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "react-native": "^0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.1.0",
    "typescript": "^5.9.2"
  },
  "private": true
}